{
  "name": "community-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:client": "ng serve $NG_ALLOWED_HOSTS",
    "start:plots": "PLOTS_ENV='development' flask --app projects/plots/app.py --debug run --port 5001",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run client:storybook",
    "build-storybook": "npm run docs:json && storybook build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@observablehq/plot": "^0.6.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-essentials": "^7.0.0-beta.15",
    "@storybook/addon-interactions": "^7.0.0-beta.15",
    "@storybook/addon-links": "^7.0.0-beta.15",
    "@storybook/angular": "^7.0.0-beta.15",
    "@storybook/blocks": "^7.0.0-beta.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/jasmine": "~4.0.0",
    "is-docker": "^3.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.15",
    "typescript": "~4.8.4"
  }
}
