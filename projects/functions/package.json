{
  "name": "@community-dashboard/functions",
  "version": "0.0.1",
  "description": "community dashboard api",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 18 || 19"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint",
    "lint:fix": "yarn run eslint:fix",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "posttest": "yarn run lint",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "nodemon --ignore ./test_db.json -r source-map-support/register .",
    "start:node": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "nate <nathan@natb1.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^5.0.7",
    "@loopback/cli": "^4.1.7",
    "@loopback/core": "^4.0.7",
    "@loopback/repository": "^5.1.2",
    "@loopback/rest": "^12.0.7",
    "@loopback/rest-explorer": "^5.0.7",
    "@loopback/service-proxy": "^5.0.7",
    "@types/jest": "^29.2.5",
    "loopback-connector-openapi": "^6.2.0",
    "nodemon": "^2.0.20",
    "openapi": "^1.0.1",
    "rxjs": "^7.8.0",
    "swagger-client": "^3.18.5",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.7",
    "@loopback/eslint-config": "^13.0.7",
    "@loopback/testlab": "^5.0.7",
    "@types/node": "^14.18.36",
    "eslint": "^8.30.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.9.4"
  }
}
